      - label: "Community"
        colour: "Community"
      - label: "Foundation"
        colour: "Foundation"
      - label: "Independent"
        colour: "Independent"
      - label: "Special School"
        colour: "Special School"
      - label: "Voluntary"
        colour: "Voluntary"
