---
title: Schools cultural engagement
config:
  geojson:
    key: "URN"
    data: viz.school-map
  data: viz.schools
  columns:
    - name: "Tooltip label"
      template: "<h3>{{ Name }}</h3><p>{{ phase_type_grouping }} (ages {{ StatutoryLowAge }}-{{ StatutoryHighAge }})<br />Pupils: {{ total_pupils | toLocaleString() }}<br />Artsmark: {{ artsmark_progress }} {{ most_recent_award }}<br />Artsaward: {{ artsaward }}</p>"
  key: "URN"
  tooltip: "Tooltip label"
  value: "total_pupils"
  scale: "Viridis"
  min: 0
  max: 2000
  legend:
    position: top right
    items:
      - value: 2000
        label: "2,000+"
      - value: 1500
        label: "1,500"
      - value: 1000
        label: "1,000"
      - value: 500
        label: "500"
      - value: 0
        label: "0"
---

{{
  comp.oi.map.zoomable({
    config: config
  }) | safe
}}

<pre>
{{ config | dump(2) }}
</pre>